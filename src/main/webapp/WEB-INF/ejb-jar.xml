<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                            http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
        version="3.1">
        <description>Enterprise JavaBeans 3.1</description>
        <display-name>Enterprise JavaBeans 3.1</display-name>
        <enterprise-beans>
                <session>
                        <ejb-name>RefreshFeedsTimerBean</ejb-name>
                        <ejb-class>cz.kec.wls.feedcombiner.beans.RefreshFeedsTimerBean</ejb-class>
<!--                        <session-type>Stateless</session-type>-->
                        <init-on-startup>true</init-on-startup>
                        <env-entry>
                                <env-entry-name>FILTER</env-entry-name>
                                <env-entry-type>java.lang.Long</env-entry-type>
                                <env-entry-value>11011</env-entry-value>
                        </env-entry>
                </session>
        </enterprise-beans>
</ejb-jar>
